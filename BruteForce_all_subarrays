public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
		int[] a = {1,3,5,2};
		int n = 4;
		System.out.println("no of sub-arrays are:"+(n*(n+1)/2));
		for(int l=0;l<4;l++)
		
		{
		    for(int r=l;r<4;r++)
		    {
		        for(int i=l;i<=r;i++)
		        {
		            System.out.print(a[i]+"  ");
		        }
		        System.out.println();
		        System.out.println("sub");
		    }
		    
		}
	}
}
